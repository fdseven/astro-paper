{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4322",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    },
    {
      "title": "page",
      "path": "[[workspace]]/src/pages"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "blog",
      "pageBundle": false,
      "previewPath": "",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "default": "FDSEVEN"
        },
        {
          "title": "Publishing date",
          "name": "pubDatetime",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Slug",
          "name": "postSlug",
          "type": "string",
          "default": "slugified title"
        },
        {
          "title": "Featured",
          "name": "isFeatured",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Tags",
          "name": "tags",
          "type": "list",
          "default": [
            "post"
          ]
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "default": "post description"
        },
        {
          "title": "Content preview",
          "name": "ogImage",
          "type": "image",
          "isPreviewImage": true,
          "default": ""
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        }
      ]
    },
    {
      "name": "page",
      "pageBundle": false,
      "previewPath": "",
      "filePrefix": null,
      "clearEmpty": true,
      "fields": [
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "default": "FDSEVEN"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Layout",
          "name": "layout",
          "type": "string",
          "single": true,
          "default": "../layouts/AboutLayout.astro"
        },
      ]
    }
  ],
  "frontMatter.website.host": "https://pesmania.web.id"
}