{
  "$schema": "https://frontmatter.codes/frontmatter.schema.json",
  "frontMatter.framework.id": "astro",
  "frontMatter.preview.host": "http://localhost:4321",
  "frontMatter.content.publicFolder": "public",
  "frontMatter.snippets.wrapper.enabled": false,
  "frontMatter.global.activeMode": "",
  "frontMatter.global.modes": [
    {
      "id": "mymode",
      "features": [
        "panel.globalSettings",
        "panel.seo",
        "panel.actions",
        "panel.metadata",
        "panel.contentType",
        "panel.recentlyModified",
        "panel.otherActions",
        "dashboard.snippets.view",
        "dashboard.snippets.manage",
        "dashboard.data.view",
        "dashboard.taxonomy.view"
      ]
    }
  ],
  "frontMatter.content.pageFolders": [
    {
      "title": "blog",
      "path": "[[workspace]]/src/content/blog"
    },
    {
      "title": "page",
      "path": "[[workspace]]/src/pages"
    },
    {
      "path": "/Volumes/FDSEVEN/WORKS/PROJECTS/2023/astro/template/mckerlie.com"
    }
  ],
  "frontMatter.taxonomy.contentTypes": [
    {
      "name": "Blog post",
      "defaultFileName": "blog-post",
      "pageBundle": false,
      "template": "[[workspace]]/.frontmatter/templates/blog-post.mdx",
      "previewPath": "/posts/",
      "filePrefix": null,
      "clearEmpty": false,
      "fileType": "mdx",
      "fields": [
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "default": "fdseven"
        },
        {
          "title": "Publishing date",
          "name": "pubDatetime",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Slug",
          "name": "postSlug",
          "type": "string",
          "editable": true,
          "default": "{{slug}}"
        },
        {
          "title": "Featured",
          "name": "featured",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "boolean",
          "default": false
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "default": "post description"
        },
        {
          "title": "Content preview",
          "name": "coverImage",
          "type": "image",
          "isPreviewImage": true,
          "default": "/assets/forrest-gump-quote.webp"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "PES",
            "PES 2021",
            "Patch",
            "FIFA",
            "Database"
          ]
        },
        {
          "title": "isFeatured",
          "name": "isFeatured",
          "type": "datetime"
        }
      ]
    },
    {
      "name": "blog video cover",
      "defaultFileName": "blog-post",
      "pageBundle": false,
      "template": "[[workspace]]/.frontmatter/templates/blog-video.mdx",
      "previewPath": "/posts/",
      "filePrefix": null,
      "clearEmpty": false,
      "fileType": "mdx",
      "fields": [
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "default": "fdseven"
        },
        {
          "title": "Publishing date",
          "name": "pubDatetime",
          "type": "datetime",
          "default": "{{now}}",
          "isPublishDate": true
        },
        {
          "title": "Last modified date",
          "name": "lastmod",
          "type": "datetime",
          "isModifiedDate": true
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true
        },
        {
          "title": "Slug",
          "name": "postSlug",
          "type": "string",
          "editable": true,
          "default": "{{slug}}"
        },
        {
          "title": "Featured",
          "name": "featured",
          "type": "boolean",
          "default": false
        },
        {
          "title": "Draft",
          "name": "draft",
          "type": "boolean",
          "default": false
        },
        {
          "title": "tags",
          "name": "tags",
          "type": "tags"
        },
        {
          "title": "Description",
          "name": "description",
          "type": "string",
          "default": "post description"
        },
        {
          "title": "Content preview",
          "name": "coverImage",
          "type": "image",
          "isPreviewImage": true,
          "default": "/assets/forrest-gump-quote.webp"
        },
        {
          "title": "Youtube Video ID",
          "name": "youtubeVideoId",
          "type": "string",
          "default": "8HC3eoMYEes"
        },
        {
          "title": "lastmod",
          "name": "lastmod",
          "type": "datetime"
        },
        {
          "title": "type",
          "name": "type",
          "type": "string"
        },
        {
          "title": "keywords",
          "name": "keywords",
          "type": "choice",
          "choices": [
            "PES",
            "Premier League",
            "Replay Wipe",
            "PES 2021"
          ]
        }
      ]
    },
    {
      "name": "page",
      "defaultFileName": "page-index",
      "pageBundle": false,
      "previewPath": "",
      "filePrefix": null,
      "clearEmpty": false,
      "fileType": "mdx",
      "fields": [
        {
          "title": "Author",
          "name": "author",
          "type": "string",
          "default": "FDSEVEN"
        },
        {
          "title": "Title",
          "name": "title",
          "type": "string",
          "single": true,
          "required": true
        },
        {
          "title": "Layout",
          "name": "layout",
          "type": "string",
          "single": true,
          "default": "../layouts/AboutLayout.astro"
        }
      ]
    }
  ],
  "frontMatter.website.host": "https://pesmania.web.id",
  "frontMatter.content.snippets": {
    "Figure": {
      "description": "Figure for image content",
      "body": "<Figure title=\"[[title]]\" src=\"[[&mediaUrl]]\" />",
      "isMediaSnippet": true
    },
    "Link Button": {
      "description": "Link style button",
      "body": [
        "<LinkButton ",
        "title=\"[[title]]\" ",
        "href=\"[[&url]]\" ",
        "className=\"[&>p]:!m-0 [&>p]:!px-4 [&>p]:!text-skin-inverted [&>p:hover]:!text-skin-accent [&>p]:bg-skin-accent [&>p]:rounded-full !decoration-transparent [&>p]:hover:bg-skin-fill [&>p]:border [&>p]:border-skin-accent [&>p]:transition\">",
        "[[title]]",
        "</LinkButton>"
      ],
      "fields": [
        {
          "name": "title",
          "title": "title",
          "type": "string",
          "single": true,
          "default": ""
        },
        {
          "name": "url",
          "title": "url",
          "type": "string",
          "single": true,
          "default": "FM_SELECTED_TEXT"
        },
        {
          "name": "title",
          "title": "title",
          "type": "string",
          "single": true,
          "default": ""
        }
      ]
    },
    "Text align": {
      "description": "Alignment for text",
      "body": "<div class=\"\\text-center\">[[title]]</div>",
      "fields": [
        {
          "name": "title",
          "title": "title",
          "type": "string",
          "single": true,
          "default": ""
        }
      ]
    },
    "Video": {
      "description": "Video content",
      "body": [
        "<video ",
        "controls ",
        ">",
        "<source ",
        "src=\"[[&mediaUrl]]\" ",
        "class=\"aspect-video\" />",
        "</video>"
      ],
      "isMediaSnippet": true
    }
  }
}